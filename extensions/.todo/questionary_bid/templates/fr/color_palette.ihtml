<script language="JavaScript1.2">
function submit()
{
	parent.window.opener.document.getElementById('template').options[0].selected = true
	parent.window.opener.document.getElementById('{ELEMENT}' + '_preview').style.backgroundColor = document.getElementById("select_code").value;
	parent.window.opener.document.getElementById('{ELEMENT}').value = document.getElementById("select_code").value;
	parent.window.close();
}
function set_color_preview(code)
{
	document.getElementById("preview_tab").style.backgroundColor = "#" + code;
	document.getElementById("preview_code").value = "#" + code;
}
function select_color(code)
{
	document.getElementById("select_tab").style.backgroundColor = "#" + code;
	document.getElementById("select_code").value = "#" + code;
}
</script>

