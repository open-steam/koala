/* ***************************************************************************************sonder Textformen*************************************************************************************** */
.caption-braun {
	text-align: center;
	font-size: 13px;
	margin: 5px;
	color: #996633;			/*BRAUN*/
}
.caption {
	text-align: center;
	font-size: 13px;
	margin: 5px;
	color: #336600;			/*GRÜN*/
}
.caption-black {
	text-align: center;
	font-size: 11px;
	margin: 5px;
	color: #000000;			/*SCHWARZ*/
}
table.standard{
	border: 1px solid #aaa;
	font-size: 13px;
	border-collapse: collapse;
	table-layout: auto;
	width: 100%;
}
table.standard tr{
	border: 1px solid #aaa;
}
table.standard th{
	text-align: center;
	border: 1px solid #aaa;
}
table.standard td{
	text-align: left;
	border: 1px solid #aaa;
	padding: 4px;
}

/* *********************************************************************************************************************************************************************************************** */

/* ***********************************************************************table für zwei Bilder mit zwei Bildunterschriften*********************************************************************** */
table.table-2ximg {
	width: 100%;
	height: 25%;
	border-spacing: 2px 0px;
}
}
table.table-2ximg tr{
	text-align: center;
}
table.table-2ximg td{
	width: 49%;
}
table.table-2ximg img{
	width: 100%;
}

/* ***********************************************************************table für drei Bilder mit drei Bildunterschriften*********************************************************************** */
table.table-3ximg {
	width: 100%;
	height: 25%;
	border-spacing: 2px 0px;
}
}
table.table-3ximg tr{
	text-align: center;
}
table.table-3ximg td{
	width: 33%;
}
table.table-3ximg img{
	width: 100%;
}
