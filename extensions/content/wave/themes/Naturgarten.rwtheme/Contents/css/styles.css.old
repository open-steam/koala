#breadcrumb, #logo, #site_title, #site_slogan {
	display: none;
}

#banner_wrapper {
	background-color: #E4F9A7;
	width: 100%;
	height: 150px;
	position: absolute;
	top: 0px;
	left: 0px;
	overflow: hidden;
	z-index: 10;
}

#content_wrapper {
	position: absolute;
	top: 150px;
	left: 0px;
	bottom: 0px;
	right: 0px;
	overflow-y: auto;
}

#navi_wrapper{
	padding-top: 0px;
	position: fixed;
	top: 150px;
	left: 0px;
	width: 200px;
	overflow-y: auto;
	font-size: 13px;
	height: 400px;
	z-index: 255;
}

#navi_wrapper ul {
	padding-left: 10px;
}

#navi_wrapper li {
	list-style: none;
}

#navi_wrapper li ul {
	display: block;
	padding-left: 10px;
}

#navi_wrapper li ul li {
	display: block;
	padding-left: 10px;
}

#navi_wrapper #current {
	font-weight: bold;
}

.banner_text {
	text-align: center;
	color: black;
	font-size: 11px;
	width: 140px;
	height: 150px;
	margin: 0px;
	padding: 0px;
	position: absolute;
}

.banner_text p {
	text-align: center;
	margin-left: 10px;
	margin-right: 10px;
}

.banner_image {
	position: absolute;
}

.banner_image img {
	height: 150px;
}

#banner_image_1 {
	top: 0px;
	left: 0px;
}

#banner_text_1 {
	top: 0px;
	left: 200px;
}

#banner_text_1 p {
	margin-top: 10px;
}

#banner_image_2 {
	top: 0px;
	left: 340px;
}

#banner_text_2 {
	top: 0px;
	left: 540px;
}

#banner_text_2 p {
	margin-top: 20px;
}

#banner_image_3 {
	top: 0px;
	left: 680px;
}

#banner_text_3 {
	top: 0px;
	left: 880px;
}

#banner_text_3 p {
	margin-top: 20px;
}

#banner_image_4 {
	top: 0px;
	left: 1020px;
}

#topline_wrapper {
	display: block;
	position: fixed;
	top: 150px;
	left: 0px;
	z-index: 10;
	height: 30px;
	width: 1020px;
	background-color: transparent;
}

#topline_content {
	
}

#navi-table {
	width: 780px;
	margin-left: 220px;
	margin-top: 5px;
}

#clear-content {
		clear: both;
}

a:link { color: #336600; text-decoration:none; }
a:visited { color: #336600; text-decoration:none; }
a:link:hover, a:visited:hover { color: #336600; text-decoration:underline; }
a:active { color: #336600; text-decoration:none; }

a img {
	border: none;
}

html, body {
	overflow: hidden;
}

body{
	background-color: #fffdef;
	font-size:13px;
	font-family: 'Lucida Grande', Verdana, Arial, Helvetica, sans-serif;
}

.book-content {
    background-color: #fffdef;
	margin-left: 0px;
	width: 779px;
	//-moz-box-shadow:2px 2px 5px black;
	//-webkit-box-shadow:2px 2px 5px black;
	padding-left: 220px;
	padding-right: 20px;
	padding-bottom: 20px;
	border: 1px solid #cccccc;
}

#footer_wrapper {
	font-size: xx-small;
	padding-left: 220px;
	padding-top: 10px;
}

#edit_mode_textarea {
	width:100%;
	height:600px
}

p {
